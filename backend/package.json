{
  "name": "claude4free-api",
  "version": "2.0.0",
  "description": "Claude4Free 多功能AI API服務",
  "main": "api-server.js",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "deploy:dev": "wrangler deploy --env development",
    "deploy:prod": "wrangler deploy --env production",
    "dev": "wrangler dev --env development",
    "tail:dev": "wrangler tail claude4free-api-dev",
    "tail:prod": "wrangler tail claude4free-api"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.0.0",
    "vitest": "^1.0.0",
    "wrangler": "^4.18.0"
  },
  "keywords": [
    "ai",
    "claude",
    "gpt",
    "api",
    "cloudflare-workers",
    "serverless"
  ],
